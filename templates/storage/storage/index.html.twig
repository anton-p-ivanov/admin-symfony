{% extends 'spreadsheet.html.twig' %}

{% block title %}File storage{% endblock %}

{% block spreadsheet %}
    {{ include('storage/storage/_index.html.twig') }}
{% endblock %}

{% block modals %}
    {{ parent() }}

    {# Modal displays upload progress #}
    {{ include('storage/storage/_upload.html.twig') }}

    {# Access modal #}
    <div class="modal" id="access-modal">
        <div class="modal__container"></div>
    </div>
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
    {{ encore_entry_script_tags('storage') }}
{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('app') }}
    {{ encore_entry_link_tags('storage') }}
{% endblock %}